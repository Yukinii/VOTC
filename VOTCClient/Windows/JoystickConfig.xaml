<Window x:Class="VOTCClient.Windows.JoystickConfig"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="JoystickConfig" Height="300" Width="300" Closing="Window_Closing">
    <Grid>
        <ProgressBar Maximum="1000" Minimum="-1000" Name="X" HorizontalAlignment="Left" Height="10" Margin="10,249,0,0" VerticalAlignment="Top" Width="272"/>
        <ProgressBar Maximum="1000" Minimum="-1000" Name="Y" HorizontalAlignment="Left" Height="234" Margin="10,10,0,0" VerticalAlignment="Top" Width="11" Orientation="Vertical"/>
        <ProgressBar Maximum="1000" Minimum="-1000" Name="Z" HorizontalAlignment="Left" Height="32" Margin="26,212,0,0" VerticalAlignment="Top" Width="256"/>
        <ProgressBar Maximum="1000" Minimum="-1000" Name="R" HorizontalAlignment="Left" Height="8" Margin="0,102,-15,0" VerticalAlignment="Top" Width="307" RenderTransformOrigin="0.5,0.5">
            <ProgressBar.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="37.53"/>
                    <TranslateTransform/>
                </TransformGroup>
            </ProgressBar.RenderTransform>
        </ProgressBar>
        <Label Name="Button" Content="" HorizontalAlignment="Left" Margin="179,21,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
